<%- include('partials/header') %>
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,400;0,700;0,800;1,400;1,700&display=swap"
        rel="stylesheet">

    <style>
        @keyframes revealUp {
            from {
                opacity: 0;
                transform: translateY(30px) scale(0.98);
            }

            to {
                opacity: 1;
                transform: translateY(0) scale(1);
            }
        }

        @keyframes subtleFloat {

            0%,
            100% {
                transform: translateY(0);
            }

            50% {
                transform: translateY(-5px);
            }
        }

        .animate-reveal {
            animation: revealUp 1s cubic-bezier(0.2, 0.8, 0.2, 1) forwards;
        }

        .animate-float-slow {
            animation: subtleFloat 4s ease-in-out infinite;
        }

        .quote-style {
            font-family: 'Montserrat', sans-serif;
            letter-spacing: -0.01em;
        }

        /* Sophisticated Floating Bloom Effect */
        .advanced-hover {
            transition: all 0.7s cubic-bezier(0.23, 1, 0.32, 1);
            position: relative;
            background: #0A2040;
            border: 1px solid rgba(255, 255, 255, 0.03);
            cursor: pointer;
        }

        .advanced-hover::before {
            content: '';
            position: absolute;
            inset: 0;
            background: radial-gradient(circle at center, rgba(0, 163, 224, 0.15) 0%, transparent 70%);
            opacity: 0;
            transition: opacity 0.7s ease;
        }

        .advanced-hover:hover {
            transform: translateY(-12px) scale(1.015);
            background: #0c254a;
            box-shadow:
                0 40px 100px -20px rgba(0, 0, 0, 0.7),
                0 20px 50px -15px rgba(0, 163, 224, 0.3),
                inset 0 0 30px rgba(255, 255, 255, 0.02);
            border-color: rgba(0, 163, 224, 0.3);
        }

        .advanced-hover:hover::before {
            opacity: 1;
        }

        .quote-text {
            transition: all 0.6s ease;
        }

        .advanced-hover:hover .quote-text {
            text-shadow: 0 0 25px rgba(255, 255, 255, 0.15);
        }

        .golden-glow {
            transition: all 0.6s cubic-bezier(0.4, 0, 0.2, 1);
        }

        .advanced-hover:hover .golden-glow {
            color: #fff;
            background: #FFD700;
            -webkit-background-clip: text;
            background-clip: text;
            text-shadow: 0 0 20px rgba(255, 215, 0, 0.6), 0 0 40px rgba(255, 215, 0, 0.2);
            transform: scale(1.02);
            display: inline-block;
        }

        /* --- Advanced Premium Transitions --- */
        .premium-card {
            transition: all 0.5s cubic-bezier(0.23, 1, 0.32, 1);
            position: relative;
            z-index: 1;
            border-left: 8px solid #0A2040 !important;
        }

        .premium-card:hover {
            transform: translateY(-8px);
            box-shadow: 0 40px 80px -15px rgba(0, 163, 224, 0.15);
        }

        .premium-card::before {
            content: '';
            position: absolute;
            inset: 0;
            border-radius: inherit;
            padding: 2px;
            background: linear-gradient(45deg, transparent, rgba(0, 163, 224, 0.3), transparent);
            -webkit-mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
            mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
            -webkit-mask-composite: xor;
            mask-composite: exclude;
            opacity: 0;
            transition: opacity 0.5s ease;
        }

        .premium-card:hover::before {
            opacity: 1;
        }

        /* --- Advanced Icon Interactions --- */
        .icon-floating {
            transition: all 0.6s cubic-bezier(0.34, 1.56, 0.64, 1);
        }

        .premium-card:hover .icon-floating {
            transform: scale(1.15) rotate(8deg);
            box-shadow: 0 15px 30px -5px rgba(0, 163, 224, 0.4);
            background: linear-gradient(135deg, #00a3e0, #00c6ff);
        }

        .premium-card:hover .icon-floating svg {
            animation: iconPulse 2s infinite;
        }

        @keyframes iconPulse {

            0%,
            100% {
                transform: scale(1);
                filter: drop-shadow(0 0 0px rgba(255, 255, 255, 0));
            }

            50% {
                transform: scale(1.1);
                filter: drop-shadow(0 0 8px rgba(255, 255, 255, 0.6));
            }
        }

        /* Inner Content Border Effect */
        .content-highlight {
            transition: all 0.5s ease;
            border-left: 4px solid #0A2040;
            border-radius: 0 16px 16px 0;
        }

        .premium-card:hover .content-highlight {
            border-left-color: #00a3e0;
            background: white;
            padding-left: 2rem;
            box-shadow: inset 0 0 20px rgba(0, 163, 224, 0.05);
        }

        .magnetic-title {
            position: relative;
            cursor: default;
        }

        .magnetic-title::after {
            content: '';
            position: absolute;
            bottom: -8px;
            left: 0;
            width: 30px;
            height: 4px;
            background: #00a3e0;
            border-radius: 20px;
            transition: width 0.6s cubic-bezier(0.23, 1, 0.32, 1);
        }

        .magnetic-title:hover::after {
            width: 100%;
        }

        .depth-image {
            transition: all 0.8s cubic-bezier(0.23, 1, 0.32, 1);
        }

        .depth-image:hover {
            transform: scale(1.03) rotate(0.5deg);
            filter: brightness(1.05) contrast(1.05);
        }

        .feature-item {
            transition: all 0.4s cubic-bezier(0.25, 0.8, 0.25, 1);
        }

        .feature-item:hover {
            transform: scale(1.05);
            background: #0A2040;
            border-color: #0A2040;
        }

        .feature-item:hover * {
            color: white !important;
        }

        .feature-item:hover .icon-box {
            background: #FFD700 !important;
            color: #0A2040 !important;
            transform: rotate(360deg);
        }

        .icon-box {
            transition: all 0.6s cubic-bezier(0.23, 1, 0.32, 1);
        }

        /* --- Stylish Timeline Markers --- */
        .timeline-marker {
            transition: all 0.5s cubic-bezier(0.34, 1.56, 0.64, 1);
            filter: drop-shadow(0 4px 6px rgba(0, 0, 0, 0.1));
        }

        .milestone-group:hover .timeline-marker {
            transform: scale(1.25);
            filter: drop-shadow(0 10px 15px rgba(0, 0, 0, 0.2));
            z-index: 20;
        }

        .timeline-year-pill {
            transition: all 0.4s ease;
            z-index: 15;
            position: relative;
        }

        .milestone-group:hover .timeline-year-pill {
            transform: scale(1.1);
            letter-spacing: 0.05em;
        }

        .marker-1975 {
            background: linear-gradient(135deg, #0088cc, #004466);
        }

        .marker-1979 {
            background: linear-gradient(135deg, #0A2040, #1a3a6a);
        }

        .marker-1984 {
            background: linear-gradient(135deg, #ff4b2b, #ff416c);
        }
    </style>

